{
    "compilerOptions": {
        "outDir": "./dist/",
        "target": "es2017",
        "module": "ES6",
        "moduleResolution": "Node",
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "strict": true,
        "noImplicitAny": true,
        "skipLibCheck": true,
    }
}